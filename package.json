    "use-latest-callback": "^0.1.9"
  },
  "devDependencies": {
    "@flinbein/rollup-plugin-varhub-bundle": "github:flinbein/rollup-plugin-varhub-bundle",
    "@parcel/packager-ts": "^2.12.0",
    "@parcel/transformer-typescript-types": "^2.12.0",
    "@types/node": "^22.9.1",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "process": "^0.11.10",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
